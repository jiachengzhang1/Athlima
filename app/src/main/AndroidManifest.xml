<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.csc436.jz.sportsgroupup">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainPage"
            android:label="MainPage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CreateEventPage" />
        <activity android:name=".MyEventsPage" />
        <activity android:name=".UserPage" />
        <activity android:name=".Signup_1Page" />
        <activity android:name=".Signup_2Page" />
        <activity android:name=".Signup_3Page"></activity>
    </application>

</manifest>